<h2>Genre Name: <%= @genre.name %></h2>
<h2>Songs:
  <ul>
    <% @genre.songs.each do |song| %>
      <h5><li><a href="/songs/<%= song.slug %>"><%= song.name %> </a></li></h5>
    <%end %>
  </ul>
<h2>Artists: </h2>
  <ul>
    <% @genre.songs.each do |song| %>
      <h3><li><a href="/artists/<%= song.artist.slug %>"><%= song.artist.name %> </a></li></h3>
    <%end %>
  </ul>
