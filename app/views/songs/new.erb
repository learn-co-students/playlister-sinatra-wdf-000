<h1>Create a new song</h1>


<form action="/songs" method="POST">
  <label> Name: </label><input type="text" name="Name">
  <br>
  <label> Genres: </label>
  <br>
  <% Genre.all.each do |genre| %>

      <input type="checkbox" name="genres[]" value="<%=genre.id%>" id="<%=genre.name%>"><%=genre.name%></input>
      <% end %>
      <br>
  <label> Artist Name: </label> <input type="text" name="Artist Name">
  <br>
  <input type="submit" value="Create">

</form>