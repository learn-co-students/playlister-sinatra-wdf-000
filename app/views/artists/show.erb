<h1 class="text-center"><%= @artist.name %> Artist Page</h1>
<hr>
<h3>Songs</h3>
<ol>
  <% @artist_songs.each do |song| %>
  <li><a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
  <% end %>
</ol>
<h4>Genre(s):</h4>
<ul>
  <% @artist_genres.each do |genre| %>
  <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
  <% end %>
</ul>
