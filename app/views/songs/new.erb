<h1>New Song:</h1>

<form action="/songs" method="post">

  <label>Name:</label>
  <input type="text" name="name" id="Name">
  <br></br>

  <label>Genre:</label>
  <%Genre.all.each do |genre|%>
    <input type="checkbox" name="genres[]" id="<%= genre.name %>" value="<%=genre.id%>"><%=genre.name%></input>
  <%end%>
  <br></br>
  <label>Artist Name:</label>
  <input type="text" name="artist_name" id="Artist Name">
  <br></br>
  <input type="submit" value="Create">

</form>
