<h2>Artist Name: <%= @artist.name %></h2>
<h2>Songs:
  <ul>
    <% @artist.songs.each do |song| %>
      <h5><li><a href="/songs/<%= song.slug %>"><%= song.name %> </a></li></h5>
    <%end %>
  </ul>
<h2>Genre: </h2>
  <ul>
    <% @artist.genres.each do |genre| %>
      <h3><li><a href="/genres/<%= genre.slug %>"><%= genre.name %> </a></li></h3>
    <%end %>
  </ul>
