Edit Song 
<%= @song.name%>

<form action="/songs/<%=@song.slug%>" method="POST">
<!-- <input type="hidden" name="_method" value="PATCH"> -->
Name: <input type="text" name="song[name]" value= "<%= @song.name%>">
  <br>
Genres:
  <br>
  <% Genre.all.each do |genre| %>
  <input type="checkbox" name="song[genres]" value="<%=genre.id%>" id="<%=genre.name%>" <%= 'checked' if @song.genres.include?(genre)%>><%=genre.name%></input>
      <% end %>
      <br>
Artist Name: <input type="text" name="Artist Name">
  <br>
  <input type="submit" value="Save">

</form>