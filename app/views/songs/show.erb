<h1>Title: <%= @song.name %></h1>
<h2>Artist: <a href="/artists/<%= @artist.slug %>"><%= @artist.name %></a></h2>
<p><strong>Genres:</strong> <% @genres.each do |genre| %>
<a href="/genres/<%= genre.slug %>"><%= genre.name %></a> 
<% end %></p>

<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

